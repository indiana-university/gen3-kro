# Namespace
namespace_name=gen3-example-aws-type-1

# Common
region=us-west-2

# VPC - ALL fields (different values)
vpc_metadata_name=example-vpc
vpc_name=example-vpc
vpc_cidr=10.100.0.0/16
vpc_public_subnet1_cidr=10.100.1.0/24
vpc_public_subnet2_cidr=10.100.2.0/24
vpc_private_subnet1_cidr=10.100.10.0/24
vpc_private_subnet2_cidr=10.100.11.0/24

# EKS - ALL fields (different values)
eks_metadata_name=example-eks
eks_name=example-eks
eks_version=1.29
eks_node_instance_types=t3.large
eks_node_desired_size=3
eks_node_min_size=2
eks_node_max_size=6

# RDS - ALL fields (different values)
rds_metadata_name=example-rds
rds_name=example-rds
rds_engine=postgres
rds_engine_version=16.1
rds_instance_class=db.t3.small
rds_allocated_storage=50
rds_database_name=exampledb

# ElastiCache - ALL fields (different values)
elasticache_metadata_name=example-elasticache
elasticache_name=example-elasticache
elasticache_engine=redis
elasticache_engine_version=7.1
elasticache_node_type=cache.t3.small
elasticache_num_cache_nodes=2

# OpenSearch - ALL fields (different values)
opensearch_metadata_name=example-opensearch
opensearch_name=example-opensearch
opensearch_domain_name=example-opensearch-domain
opensearch_version=2.13
opensearch_instance_type=t3.medium.search
opensearch_instance_count=2
opensearch_volume_size=20

# DataBucket - ALL fields (different values)
data_bucket_metadata_name=example-databucket
data_bucket_name=example-data-bucket
data_bucket_bucket_name=example-data-bucket-unique
data_bucket_versioning=true
data_bucket_encryption=true
data_bucket_public_access=false

# KMS - ALL fields (different values)
kms_metadata_name=example-kms
kms_name=example-kms
kms_description=Example KMS key for production encryption
kms_key_rotation=true

# EFS - ALL fields (different values)
efs_metadata_name=example-efs
efs_name=example-efs
efs_performance_mode=maxIO
efs_throughput_mode=provisioned
efs_encrypted=true
